{
  "name": "tweener",
  "version": "0.1.27",
  "description": "Minimal Tweening Library for Games.",
  "author": "Mauro de Tarso <maurodetarso@gmail.com>",
  "contributors": [
    "Endel Dreyer <endel.dreyer@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/maurodetarso/tweener.git"
  },
  "main": "dist/tweener.min.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "server": "http-server -p 8933",
    "dist-debug": "browserify source/index.js -s Tweener -t [ babelify --stage 0 ] -o dist/tweener.js -d",
    "dist-release": "browserify source/index.js -s Tweener -t [ babelify --stage 0 ] | uglifyjs -mc > dist/tweener.min.js",
    "dist": "npm run dist-debug & npm run dist-release",
    "start": "npm run server & watchify source/index.js -s Tweener -t [ babelify --stage 0 ] -o dist/tweener.js -dv",
    "test": "mocha tests",
    "docs": "jsdoc -c ./docs-config.json -R ./README.md -d ./docs",
    "jsdoc": "jsdoc ./source/Tweener.js",
    "prepublish": "npm run dist-release"
  },
  "license": "MIT",
  "dependencies": {
    "eases": "^1.0.6"
  },
  "devDependencies": {
    "babelify": "^6.0.2",
    "browserify": "^10.1.2",
    "chai": "^2.3.0",
    "jsdoc": "^3.2.2",
    "mocha": "^2.2.4",
    "uglify-js": "^2.4.19",
    "watchify": "^3.1.0",
    "http-server": "^0.8.0"
  }
}
