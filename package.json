{
  "name": "tweener",
  "version": "0.1.0",
  "description": "Just a minimal tween library.",
  "author": "Mauro de Tarso <maurodetarso@gmail.com>",
  "contributors": [
    "Endel Dreyer <endel.dreyer@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/maurodetarso/tweener.git"
  },
  "main": "source/Tweener.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "debug": "browserify source/Tweener.js -t [ babelify --stage 0 ] -o dist/tweener.js -d",
    "release": "browserify source/Tweener.js -t [ babelify --stage 0 ] | uglifyjs -mc > dist/tweener.min.js",
    "start": "watchify source/Tweener.js -t [ babelify --stage 0 ] -o dist/tweener.js -dv",
    "test": "mocha tests",
    "docs": "jsdoc -c ./docs-config.json -R ./README.md -d ./docs"
  },
  "license": "MIT",
  "dependencies": {
    "eases": "^1.0.6"
  },
  "devDependencies": {
    "mocha": "^2.2.4",
    "chai": "^2.3.0",
    "browserify": "^9.0.8",
    "babelify": "^6.0.2",
    "watchify": "^3.1.0",
    "jsdocs": "^0.1.0",
    "uglify-js": "^2.4.19"
  }
}
