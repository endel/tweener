{
  "name": "tweener",
  "version": "0.1.24",
  "description": "Just a minimal tween library.",
  "author": "Mauro de Tarso <maurodetarso@gmail.com>",
  "contributors": [
    "Endel Dreyer <endel.dreyer@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/maurodetarso/tweener.git"
  },
  "main": "dist/tweener.min.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dist-dev": "browserify source/index.js -s Tweener -t [babelify] -o dist/tweener.js -d",
    "dist-min": "browserify source/index.js -s Tweener -t [babelify] | uglifyjs -mc > dist/tweener.min.js",
    "dist-ease": "browserify source/ease.js -s Ease -t [babelify] | uglifyjs -mc > dist/ease.min.js",
    "dist": "npm run dist-dev & npm run dist-min & npm run dist-ease",
    "start": "watchify source/index.js -s Tweener -t [ babelify --stage 0 ] -o dist/tweener.js -dv",
    "test": "mocha --compilers js:babel-core/register --es_staging -b",
    "docs": "jsdoc -c ./docs-config.json -R ./README.md -d ./docs",
    "jsdoc": "jsdoc ./source/Tweener.js"
  },
  "license": "MIT",
  "dependencies": {
    "eases": "^1.0.6"
  },
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.15",
    "babel-core": "^6.3.15",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^6.0.2",
    "browserify": "^10.1.2",
    "chai": "^3.4.1",
    "jsdoc": "^3.2.2",
    "mocha": "^2.3.4",
    "uglify-js": "^2.4.19",
    "watchify": "^3.1.0"
  }
}
